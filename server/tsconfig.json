{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"outDir": "../dist/server",
		"rootDir": "./src",
		"module": "commonjs",
		"target": "ES2020",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "../node_modules"]
}

